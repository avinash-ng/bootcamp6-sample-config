{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": false,
        "module": "esnext",
        "target": "es5",
        "jsx": "react",
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "lib": [
            "dom",
            "es2015"
        ],
        "moduleResolution": "node",
        "types": [
            "node",
            "webpack-env",
        ]
    },
    "include": [
        "./src/*"
    ],
    "exclude": [
        "node_modules"
    ]
}